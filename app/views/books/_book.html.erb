<tr>
  <td>
  	<%= link_to user_path(book.user_id) do %>
		<%= attachment_image_tag book.user, :profile_image, :fill, 40, 40, format: 'jpeg' %>
	<% end %>
  </td>
  <td>
    <%=link_to book.title, book_path(book) %>
  </td>
  <td>
	<%= book.body %>
  </td>
		    <td id="favorite-<%= book.id %>">
		    	<%= render partial: "favorite", locals: { book: book } %>
			  	
			</td>
		  

</tr>
